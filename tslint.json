{
    "defaultSeverity": "error",
    "extends": ["tslint-config-airbnb"],
    "jsRules": {},
    "rules": {
      "import-name": [false],
      "variable-name": [
        true,
        "ban-keywords",
        "check-format",
        "allow-pascal-case"
      ],
      "quotemark": [true, "single", "avoid-escape", "avoid-template"]
    },
    "rulesDirectory": []
  }